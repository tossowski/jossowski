        <script>
            Galleria.loadTheme('galleria/themes/classic/galleria.classic.min.js');
Galleria.configure({
    lightbox: true,
    showInfo: true,
    imagePan: true,
    thumbnails: false
});
            Galleria.run('#galleria', {

   dataConfig: function(img) {
        return {
            description: $(img).next('p').html()
        }
    },

    extend: function(options) {
        Galleria.get(0).$('info-link').click();
    }

});
        </script>
